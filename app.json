{
  "expo": {
    "name": "MyAudaxUK",
    "slug": "MyAudaxUK",
    "privacy": "public",
    "ios": {
      "bundleIdentifier": "com.andytrich.MyAudaxUK",
      "supportsTablet": true
    },
    "android": {
      "package": "com.andytrich.MyAudaxUK",
      "versionCode": 7
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": "Access your Audax UK calendar and history as well as view rides coming up"
  }
}
